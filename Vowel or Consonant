#include<stdio.h>
#include<ctype.h>

int main(){

    char ch;
    scanf("%c", &ch);

    if (!isalpha(ch)){
        printf("Not an alphabet\n");
    }

    else{
        if(ch=='a'||ch=='A'||ch=='e'||ch=='E'||ch=='i'||ch=='I'||ch=='o'||ch=='O'||ch=='u'||ch=='U'){
            printf("%c is a vowel", ch);
        }
        else{
            printf("%c is a consonant", ch);
        }
        }
    
}
